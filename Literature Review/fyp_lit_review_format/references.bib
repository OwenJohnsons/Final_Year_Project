@article{BurkeSpolaor:2010p5814,
author = {Burke-Spolaor, Sarah and Bailes, Matthew and Ekers, Ronald and Macquart, Jean-Pierre and Crawford III, Fronefield},
title = {{Radio Bursts with Extragalactic Spectral Characteristics Show Terrestrial Origins}},
journal = {arXiv.org},
year = {2010},
eprint = {1009.5392v1},
eprinttype = {arxiv},
eprintclass = {astro-ph.CO},
month = sep,
annote = {2 figures, 1 table, accepted to Astrophysical Journal Letters},
keywords = {astro-ph.CO, astro-ph.EP},
read = {Yes},
rating = {0},
date-added = {2011-01-06T05:26:59GMT},
date-modified = {2013-06-15T18:52:07GMT},
abstract = {Three years ago, the report of a solitary radio burst was thought to be the first discovery of a rare, impulsive event of unknown extragalactic origin (Lorimer et al. 2007). The extragalactic interpretation was based on the swept-frequency nature of the event, which followed the dispersive delay expected from an extragalactic pulse. We report here on the detection of 16 pulses, the bulk of which exhibit a frequency sweep with a shape and magnitude resembling the Lorimer Burst. These new events were detected in a sidelobe of the Parkes Telescope and are of clearly terrestrial origin, with properties unlike any known sources of terrestrial broad-band radio emission. The new detections cast doubt on the extragalactic interpretation of the original burst, and call for further sophistication in radio-pulse survey techniques to identify the origin of the anomalous terrestrial signals and definitively distinguish future extragalactic pulse detections from local signals. The ambiguous origin of these seemingly dispersed, swept-frequency signals suggest that radio-pulse searches using multiple detectors will be the only experiments able to provide definitive information about the origin of new swept-frequency radio burst detections.},
url = {http://arxiv.org/abs/1009.5392v1},
local-url = {file://localhost/Users/siemion/Documents/Papers2/Articles/2010/Burke-Spolaor/arXiv%202010%20Burke-Spolaor-1.pdf},
file = {{arXiv 2010 Burke-Spolaor-1.pdf:/Users/siemion/Documents/Papers2/Articles/2010/Burke-Spolaor/arXiv 2010 Burke-Spolaor-1.pdf:application/pdf}},
uri = {\url{papers2://publication/uuid/EF1637D1-0BB8-49B6-A9C9-69C7136B05B4}}
}

