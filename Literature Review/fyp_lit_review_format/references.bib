file = {{arXiv 2012 Mickaliger.pdf:/Users/siemion/Documents/Papers2/Articles/2012/Mickaliger/arXiv 2012 Mickaliger.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1088/0004-637X/760/1/64}}
}

