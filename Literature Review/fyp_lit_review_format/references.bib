volume = {705},
pages = {1252},
month = nov,
affiliation = {AA(Department of Astronomy, Nanjing University, Nanjing 210093, China ; Center for Gravitational Wave Astronomy and Department of Physics and Astronomy, University of Texas at Brownsville, Brownsville, Texas 78520, USA), AB(Center for Gravitational Wave Astronomy and Department of Physics and Astronomy, University of Texas at Brownsville, Brownsville, Texas 78520, USA), AC(Center for Gravitational Wave Astronomy and Department of Physics and Astronomy, University of Texas at Brownsville, Brownsville, Texas ...},
