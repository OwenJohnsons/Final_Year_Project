month = jul,
affiliation = {AA(Optical Technology Division, National Institute of Standards and Technology, Gaithersburg, MD 20899.), AB(NASA Goddard Space Flight Center, Computational and Information Sciences and Technology Office, Code 606, Greenbelt, MD 20771.), AC(National Radio Astronomy Observatory, 520 Edgemont Road, Charlottesville, VA 22903-2475.), AD(National Radio Astronomy Observatory, 520 Edgemont Road, Charlottesville, VA 22903-2475.)},
doi = {10.1086/506324},
read = {Yes},
