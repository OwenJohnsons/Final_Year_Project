@article{Deneva:2009p9095,
author = {Lazio, T Joseph W and Cordes, James M},
title = {{Hyperstrong Radio‐Wave Scattering in the Galactic Center. II. A Likelihood Analysis of Free Electrons in the Galactic Center}},
journal = {The Astrophysical Journal},
year = {1998},
volume = {505},
number = {2},
pages = {715--731},
month = oct,
affiliation = {AA(Astronomy Department, Cornell University, Ithaca, NY 14853, USA ), AB(Astronomy Department, Cornell University, Ithaca, NY 14853, USA ), AC(Naval Research Laboratory, 4555 Overlook Avenue, SW, Washington, DC 20375, USA)},
