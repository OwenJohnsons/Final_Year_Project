title = {{Ground-Based and Space-Based Radio Observations of Planetary Lightning}},
journal = {Planetary Atmospheric Electricity},
year = {2008},
volume = {137},
pages = {257},
affiliation = {AA(Observatoire de Paris, CNRS, UPMC, Universit{\'e} Paris Diderot), AB(Science Exploration Directorate, NASA Goddard Space Flight Center), AC(Dept. of Physics and Astronomy, University of Iowa), AD(Institute of Radio Astronomy, National Academy of Sciences of Ukraine)},
