file = {{arXiv 2006 Johnston.pdf:/Users/siemion/Documents/Papers2/Articles/2006/Johnston/arXiv 2006 Johnston.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1111/j.1745-3933.2006.00232.x}}
}

@article{Li:2010ug,
author = {Li, Juan and An, Tao and Shen, Zhi-Qiang and Miyazaki, Atsushi},
title = {{ATCA observations of SiO masers in the Galactic center}},
journal = {arXiv.org},
year = {2010},
eprint = {1007.4788v1},
eprinttype = {arxiv},
eprintclass = {astro-ph.GA},
month = jul,
annote = {15 pages, 3 figures, accepted by ApJ Letter},
rating = {0},
date-added = {2012-04-09T18:41:09GMT},
date-modified = {2012-05-25T19:43:00GMT},
abstract = {We present the Australia Telescope Compact Array (ATCA) observations of the SiO masers in the Galactic center in transitions of v=1, J=2--1 at 86 GHz and v=1, J=1--0 at 43 GHz. Two 86-GHz SiO masers were detected within the central parsec, and they are associated with IRS 10EE and IRS 15NE, respectively. We detected eighteen 43-GHz SiO masers within a projected separation of <2 pc from Sagittarius A* (Sgr A*), among which seven masers are newly discovered from our observations. This raises the total number of 43-GHz SiO masers within the central 4 parsecs of the GC region to 22. Simultaneous observations at 86 and 43 GHz showed that the intensity of 43-GHz SiO maser is ~3 times higher than that of 86-GHz maser in IRS 10EE (an OH/IR star), while the integrated flux of the SiO maser emission at 43 GHz is comparable with that at 86~GHz in IRS~15NE (an ordinary Mira variable). These results are consistent with previous observations of massive late-type stars in the Galaxy in which the 86-GHz SiO maser is in general weaker than the 43-GHz SiO maser in OH/IR stars, while the two transitions are comparably strong in Mira stars.},
url = {http://arxiv.org/abs/1007.4788v1},
local-url = {file://localhost/Users/siemion/Documents/Papers2/Articles/2010/Li/arXiv%202010%20Li.pdf},
file = {{arXiv 2010 Li.pdf:/Users/siemion/Documents/Papers2/Articles/2010/Li/arXiv 2010 Li.pdf:application/pdf}},
uri = {\url{papers2://publication/uuid/10B6D4CB-9A76-4AC5-B9A7-FD990C3E1E95}}
}

