file = {{The Astrophysical Journal 2013 Siemion.pdf:/Users/siemion/Documents/Papers2/Articles/2013/Siemion/The Astrophysical Journal 2013 Siemion.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1088/0004-637X/767/1/94}}
}

