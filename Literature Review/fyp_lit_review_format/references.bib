month = dec,
affiliation = {AA(Astro Space Center of Lebedev Physical Institute, Leninsky Prospect 53, Moscow 117924, Russia; mpopov@asc.rssi.ru) AB(Astro Space Center of Lebedev Physical Institute, Leninsky Prospect 53, Moscow 117924, Russia; vsoglasn@asc.rssi.ru) AC(Astro Space Center of Lebedev Physical Institute, Leninsky Prospect 53, Moscow 117924, Russia; West Virginia University, Department of Physics, Morgantown, WV 26506-6315, USA; National Radio Astronomy Observatory, 520 Edgemont Road, Charlottesville, VA 22903-2475, USA; v...},
rating = {0},
date-added = {2011-02-08T09:00:49GMT},
date-modified = {2016-05-18T05:17:59GMT},
abstract = {Simultaneous multifrequency observations of Crab pulsar giant pulses (GPs) were performed with the 64-m Kalyazin radio telescope at four frequency ($\nu$ = 0.6, 1.4, 2.2, and 8.3 GHz) using the K5 VLBI recording terminal. The K5 terminal provided continuous recording in 16 × 4-MHz wide frequency channels distributed over 4 frequency bands. Several thousand GPs were detected during about 6 hours of observations on two successive days in 2005 July. The radio spectra of single GPs were analyzed at separate frequencies and over the whole frequency range. These spectra manifest notable modulation over the frequency ranges, $\Delta$$\nu$, both on large ($\Delta$$\nu$/$\nu$ ≈ 0.5) and small ($\Delta$$\nu$/$\nu$ ≈ 0.01) frequency scales. A cross-correlation analysis of GPs at 2.2 GHz showed that their pulse shapes can be interpreted as being an ensemble of unresolved bursts grouped together on time scales of ≈1$\mu$s, being well-correlated over a 60-MHz band. The corresponding GP cross-correlation functions do not obey the predictions of the amplitude-modulated noise model of <A >Rickett(1975)</A>, thus indicating that unresolved components represent a small number of elementary emitters.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2009PASJ...61.1197P&link_type=ABSTRACT},
local-url = {file://localhost/Users/siemion/Documents/Papers2/Articles/2009/Popov/Publications%20of%20the%20Astronomical%20Society%20of%20Japan%202009%20Popov.pdf},
file = {{Publications of the Astronomical Society of Japan 2009 Popov.pdf:/Users/siemion/Documents/Papers2/Articles/2009/Popov/Publications of the Astronomical Society of Japan 2009 Popov.pdf:application/pdf}},
uri = {\url{papers2://publication/uuid/E071AAD3-067E-4124-ADF0-8B91477C64FF}}
}

@article{Johnston:2004p9517,
author = {Johnston, Simon and Romani, Roger W and Marshall, F E and Zhang, W},
title = {{Radio and X-ray observations of PSR B0540-69}},
journal = {Mon. Not. R. Astron. Soc.},
year = {2004},
volume = {355},
pages = {31},
month = nov,
affiliation = {AA(School of Physics, University of Sydney, NSW 2006, Australia), AB(Department of Physics, Stanford University, Stanford, CA 94305-4060, USA), AC(Laboratory for High-energy Astrophysics, Goddard Space Flight Center, Greenbelt, MD 20771, USA), AD(Laboratory for High-energy Astrophysics, Goddard Space Flight Center, Greenbelt, MD 20771, USA)},
doi = {10.1111/j.1365-2966.2004.08286.x},
rating = {0},
date-added = {2011-02-08T09:41:05GMT},
date-modified = {2016-05-18T05:17:59GMT},
abstract = {PSR B0540-69 is one of a small handful of pulsars known to emit giant pulses and the only extra-galactic pulsar known to do so. We observed the pulsar for a total of 72 h over a 6 month interval and detected 141 giant pulses. We have obtained correct phasing between the radio arrival times of the giant pulses and the X-ray pulse profile. The giant pulses occur in two phase windows, located 6.7 ms before and 5.0 ms after the mid-point of the X-ray profile. We have detected the integrated profile of the pulsar at 1.4 GHz with a flux density of only 24 $\mu$Jy. The statistics of the giant pulses are clearly power law and it is likely that the giant pulses contribute only a few per cent of the integrated pulse flux density, similar to other pulsars with giant pulse emission. Simultaneous X-ray and radio observations show no significant increase in the X-ray flux at the time of the radio giants. The relative enhancements in the X-ray emission must be at least 5.5 times smaller than in the radio emission.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2004MNRAS.355...31J&link_type=ABSTRACT},
local-url = {file://localhost/Users/siemion/Documents/Papers2/Articles/2004/Johnston/Mon.%20Not.%20R.%20Astron.%20Soc.%202004%20Johnston.pdf},
file = {{Mon. Not. R. Astron. Soc. 2004 Johnston.pdf:/Users/siemion/Documents/Papers2/Articles/2004/Johnston/Mon. Not. R. Astron. Soc. 2004 Johnston.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1111/j.1365-2966.2004.08286.x}}
}

