@article{2011MNRAS.415....2B,
author = {Bell, M E and Fender, R P and Swinbank, J and Miller-Jones, J C A and Law, C J and Scheers, B and Spreeuw, H and Wise, M W and Stappers, B W and Wijers, R A M J and Hessels, J W T and Masters, J},
title = {{An automated archival Very Large Array transients survey}},
