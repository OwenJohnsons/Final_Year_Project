@article{2002ASPC..278..525B,
author = {Backus, P R and Team, Project Phoenix},
title = {{Project Phoenix: SETI Observations from 1200 to 1750 MHz with the Upgraded Arecibo Telescope}},
journal = {Single-Dish Radio Astronomy: Techniques and Applications},
year = {2002},
volume = {278},
pages = {525--527},
