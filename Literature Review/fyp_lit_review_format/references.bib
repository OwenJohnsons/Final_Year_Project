@article{2009A&A...502...91S,
author = {Sch{\"o}del, R. and Merritt, D. and Eckart, A.},
title = {{The nuclear star cluster of the Milky Way: proper motions and mass}},
journal = {Astronomy and Astrophysics},
year = {2009},
volume = {502},
number = {1},
pages = {91--111},
month = jul,
affiliation = {Instituto de Astrof{\'\i}sica de Andaluc{\'\i}a (IAA) - CSIC, Camino Bajo de Hu{\'e}tor 50, 18008 Granada, Spain rainer@iaa.es},
doi = {10.1051/0004-6361/200810922},
read = {Yes},
rating = {0},
date-added = {2012-02-28T18:09:54GMT},
date-modified = {2013-07-01T12:46:14GMT},
abstract = {Context: Nuclear star clusters (NSCs) are located at the photometric and dynamical centers of the majority of galaxies. They are among the densest star clusters in the Universe. The NSC in the Milky Way is the only object of this class that can be resolved into individual stars. The massive black hole Sagittarius A* is located at the dynamical center of the Milky Way NSC. Aims: In this work we examine the proper motions of stars out to distances of 1.0 pc from Sgr A*. The aim is to examine the velocity structure of the MW NSC and acquire a reliable estimate of the stellar mass in the central parsec of the MW NSC, in addition to the well-known black hole mass. Methods: We use multi-epoch adaptive optics assisted near-infrared observations of the central parsec of the Galaxy obtained with NACO/CONICA at the ESO VLT. Stellar positions are measured via PSF fitting in the individual images and transformed into a common reference frame via suitable sets of reference stars. Results: We measured the proper motions of more than 6000 stars within ~1.0 pc of Sagittarius A*. The full data set is provided in this work. We largely exclude the known early-type stars with their peculiar dynamical properties from the dynamical analysis. The cluster is found to rotate parallel to Galactic rotation, while the velocity dispersion appears isotropic (or anisotropy may be masked by the cluster rotation). The Keplerian fall-off of the velocity dispersion due to the point mass of Sgr A* is clearly detectable only at R ⪉ 0.3 pc. Nonparametric isotropic and anisotropic Jeans models are applied to the data. They imply a best-fit black hole mass of 3.6^+0.2-0.4 × 106 M{\&}sun;. Although this value is slightly lower than the current canonical value of 4.0 × 106 M{\&}sun;, this is the first time that a proper motion analysis provides a mass for Sagittarius A* that is consistent with the mass inferred from orbits of individual stars. The point mass of Sagittarius A* is not sufficient to explain the velocity data. In addition to the black hole, the models require the presence of an extended mass of 0.5-1.5 × 106 M{\&}sun; in the central parsec. This is the first time that the extended mass of the nuclear star cluster is unambiguously detected. The influence of the extended mass on the gravitational potential becomes notable at distances ⪆0.4 pc from Sgr A*. Constraints on the distribution of this extended mass are weak. The extended mass can be explained well by the mass of the stars that make up the cluster. Appendices are only available in electronic form at http://www.aanda.org Full Table [see full text] is only available in electronic form at the CDS via anonymous ftp to cdsarc.u-strasbg.fr (130.79.128.5) or via http://cdsweb.u-strasbg.fr/cgi-bin/qcat?J/A+A/502/91},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2009A%26A...502...91S&link_type=ABSTRACT},
local-url = {file://localhost/Users/siemion/Documents/Papers2/Articles/2009/Sch%C3%B6del/Astronomy%20and%20Astrophysics%202009%20Sch%C3%B6del.pdf},
file = {{Astronomy and Astrophysics 2009 Schödel.pdf:/Users/siemion/Documents/Papers2/Articles/2009/Schödel/Astronomy and Astrophysics 2009 Schödel.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1051/0004-6361/200810922}}
}

