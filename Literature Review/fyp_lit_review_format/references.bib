@article{Sjouwerman:2010vy,
author = {Sjouwerman, Lorant O and Murray, Claire E and Pihlstrom, Ylva M and Fish, Vincent L and Araya, Esteban D},
title = {{Discovery of the first methanol (CH3OH) maser in the Andromeda galaxy (M31)}},
journal = {arXiv.org},
year = {2010},
eprint = {1010.4818v1},
eprinttype = {arxiv},
eprintclass = {astro-ph.GA},
month = oct,
annote = {Letter, accepted by ApJL},
rating = {0},
date-added = {2012-04-13T11:43:12GMT},
date-modified = {2012-05-25T19:43:00GMT},
abstract = {We present the first detection of a 6.7 GHz Class II methanol (CH3OH) maser in the Andromeda galaxy (M31). The CH3OH maser was found in a Very Large Array (VLA) survey during the fall of 2009. We have confirmed the methanol maser with the new Expanded VLA (EVLA), in operation since March 2010, but were unsuccessful in detecting a water maser at this location. A direct application for this methanol maser is the determination of the proper motion of M31, such as was obtained with water masers in M33 and IC10 previously. Unraveling the three-dimensional velocity of M31 would solve for the biggest unknown in the modeling of the dynamics and evolution of the Local Group of galaxies.},
url = {http://arxiv.org/abs/1010.4818v1},
