journal = {The Astrophysical Journal},
year = {2010},
volume = {725},
number = {2},
pages = {1792--1804},
month = dec,
affiliation = {University of California, Berkeley, 601 Campbell Hall 3411, Berkeley, CA 94720, USA gbower@astro.berkeley.edu},
