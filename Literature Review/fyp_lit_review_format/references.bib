journal = {Astronomy and Astrophysics},
year = {2003},
volume = {407},
pages = {655},
month = aug,
affiliation = {AA(Jodrell Bank Observatory, University of Manchester, Macclesfield, Chesire SK11 9DL, UK), AB(Max-Planck Institut f{\"u}r Radioastronomie, Auf dem H{\"u}gel 69, 53121 Bonn, Germany; School of Physics, University of Sydney, NSW 2006, Australia), AC(NCRA, TIFR, Pune University Campus, Ganeshkind, Pune 411007, India), AD(School of Physics, University of Sydney, NSW 2006, Australia), AE(Arecibo Observatory, HC3 Box 53995, Arecibo, Puerto Rico, PR 00612, USA), AF(Jodrell Bank Observatory, University of Manchester, Macc...},
