@inproceedings{2011ASPC..439..180H,
author = {Hopman, C. and Madigan, A.},
title = {{Mass Segregation in the Galactic Centre}},
booktitle = {The Galactic Center: a Window to the Nuclear Environment of Disk Galaxies. Proceedings of a workshop held at Shanghai},
year = {2011},
pages = {180},
month = may,
read = {Yes},
