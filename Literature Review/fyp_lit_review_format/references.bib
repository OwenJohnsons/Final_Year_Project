journal = {The Astrophysical Journal Letters},
year = {2011},
volume = {728},
pages = {L14},
month = feb,
affiliation = {AA(Radio Astronomy Laboratory and Department of Astronomy, University of California, Berkeley, 601 Campbell Hall 3411, Berkeley, CA 94720, USA gbower@astro.berkeley.edu), AB(Department of Physics, Columbia University, 538 West 120th Street, 1333 Pupin, MC 5255, New York, NY 10027, USA destry@astro.columbia.edu)},
doi = {10.1088/2041-8205/728/1/L14},
rating = {0},
date-added = {2011-06-14T02:30:49GMT},
date-modified = {2016-05-18T05:17:59GMT},
abstract = {We present a search for radio transients (RTs) in the field of the bright radio source 3C 286 using archival observations from the Very Large Array (VLA). These observations span 23 years and include 1852 epochs at 1.4 GHz in the C and D configurations. We find no transients in the field. The sensitivity of the observations is limited by dynamic range effects in the images. At large flux densities (>0.2 Jy), single-epoch observations provide a strong limit on the transient surface density. At flux densities near the dynamic range threshold, we use the requirement that transient sources must appear in consecutive epochs to be confirmed as real. This sets the sensitivity at low flux densities to transient durations of $\tau$ ~ 1 day, while $\tau$>1 minute for high flux densities. At 70 mJy, we find a 2$\sigma$ limit on the surface density $\Sigma$ < 3 × 10-3 deg-2. At 3 Jy, we find a 2$\sigma$ limit $\Sigma$ < 9 × 10-4 deg-2. A future systematic search of the VLA archives can provide one to two orders of magnitude more sensitivity to RTs.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2011ApJ...728L..14B&link_type=ABSTRACT},
