pages = {78},
month = sep,
affiliation = {AA(Massachusetts Institute of Technology (MIT) Haystack Observatory, Off Route 40, Westford, Massachusetts 01886, USA), AB(Harvard-Smithsonian Center for Astrophysics, 60 Garden Street, Cambridge, Massachusetts 02138, USA), AC(Massachusetts Institute of Technology (MIT) Haystack Observatory, Off Route 40, Westford, Massachusetts 01886, USA), AD(University of California Berkeley, Department of Astronomy, 601 Campbell, Berkeley, California 94720-3411 USA), AE(Arizona Radio Observatory, Steward Observatory, Un...},
doi = {10.1038/nature07245},
rating = {0},
date-added = {2010-10-30T23:07:42GMT},
date-modified = {2016-05-18T05:17:58GMT},
abstract = {The cores of most galaxies are thought to harbour supermassive black holes, which power galactic nuclei by converting the gravitational energy of accreting matter into radiation. Sagittarius A* (SgrA*), the compact source of radio, infrared and X-ray emission at the centre of the Milky Way, is the closest example of this phenomenon, with an estimated black hole mass that is 4,000,000 times that of the Sun. A long-standing astronomical goal is to resolve structures in the innermost accretion flow surrounding SgrA*, where strong gravitational fields will distort the appearance of radiation emitted near the black hole. Radio observations at wavelengths of 3.5mm and 7mm have detected intrinsic structure in SgrA*, but the spatial resolution of observations at these wavelengths is limited by interstellar scattering. Here we report observations at a wavelength of 1.3mm that set a size of microarcseconds on the intrinsic diameter of SgrA*. This is less than the expected apparent size of the event horizon of the presumed black hole, suggesting that the bulk of SgrA* emission may not be centred on the black hole, but arises in the surrounding accretion flow.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2008Natur.455...78D&link_type=ABSTRACT},
local-url = {file://localhost/Users/siemion/Documents/Papers2/Articles/2008/Doeleman/Nature%202008%20Doeleman.pdf},
