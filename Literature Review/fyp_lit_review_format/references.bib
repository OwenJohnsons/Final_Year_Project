file = {{arXiv 2014 Shannon.pdf:/Users/siemion/Documents/Papers2/Articles/2014/Shannon/arXiv 2014 Shannon.pdf:application/pdf}},
uri = {\url{papers2://publication/uuid/0AA8A682-641B-4609-A9B2-5C9CFBBA061F}}
}

