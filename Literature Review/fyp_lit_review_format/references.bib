@article{2013ApJ...767....4T,
author = {Trott, Cathryn M and Tingay, Steven J and Wayth, Randall B and Thompson, David R and Deller, Adam T and Brisken, Walter F. and Wagstaff, Kiri L and Majid, Walid A and Burke-Spolaor, Sarah and Macquart, Jean-Pierre R and Palaniswamy, Divya},
title = {{A Framework for Interpreting Fast Radio Transients Search Experiments: Application to the V-FASTR Experiment}},
journal = {The Astrophysical Journal},
year = {2013},
volume = {767},
number = {1},
pages = {4},
month = apr,
affiliation = {International Centre for Radio Astronomy Research, Curtin University, Bentley, WA 6845, Australia ; Centre of Excellence for All-Sky Astrophysics (CAASTRO).; cathryn.trott@curtin.edu.au},
doi = {10.1088/0004-637X/767/1/4},
read = {Yes},
rating = {0},
date-added = {2013-05-12T10:21:04GMT},
date-modified = {2013-06-15T18:52:08GMT},
abstract = {We define a framework for determining constraints on the detection rate of fast transient events from a population of underlying sources, with a view to incorporate beam shape, frequency effects, scattering effects, and detection efficiency into the metric. We then demonstrate a method for combining independent data sets into a single event rate constraint diagram, using a probabilistic approach to the limits on parameter space. We apply this new framework to present the latest results from the V-FASTR experiment, a commensal fast transients search using the Very Long Baseline Array (VLBA). In the 20 cm band, V-FASTR now has the ability to probe the regions of parameter space of importance for the observed Lorimer and Keane fast radio transient candidates by combining the information from observations with differing bandwidths, and properly accounting for the source dispersion measure, VLBA antenna beam shape, experiment time sampling, and stochastic nature of events. We then apply the framework to combine the results of the V-FASTR and Allen Telescope Array Fly's Eye experiments, demonstrating their complementarity. Expectations for fast transients experiments for the SKA Phase I dish array are then computed, and the impact of large differential bandwidths is discussed.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2013ApJ...767....4T&link_type=ABSTRACT},
local-url = {file://localhost/Users/siemion/Documents/Papers2/Articles/2013/Trott/The%20Astrophysical%20Journal%202013%20Trott.pdf},
file = {{The Astrophysical Journal 2013 Trott.pdf:/Users/siemion/Documents/Papers2/Articles/2013/Trott/The Astrophysical Journal 2013 Trott.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1088/0004-637X/767/1/4}}
}

@article{2012ApJ...753L..36W,
author = {Wayth, Randall B and Tingay, Steven J and Deller, Adam T and Brisken, Walter F. and Thompson, David R and Wagstaff, Kiri L and Majid, Walid A},
