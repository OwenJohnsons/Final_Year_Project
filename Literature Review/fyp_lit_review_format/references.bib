uri = {\url{papers2://publication/doi/10.1111/j.1365-2966.2011.18160.x}}
}

@article{Leaman:2011p11630,
author = {Leaman, Jesse and Li, Weidong and Chornock, Ryan and Filippenko, Alexei V},
