@article{Lorimer:2005ex,
author = {Lorimer, D R and Xilouris, K M and Fruchter, A S and Stairs, I H and Camilo, F and Vazquez, A M and Eder, J A and McLaughlin, M A and Roberts, M S E and Hessels, J W T and Ransom, S M},
title = {{Discovery of 10 pulsars in an Arecibo drift-scan survey}},
journal = {arXiv.org},
year = {2005},
eprint = {astro-ph/0504019v1},
eprinttype = {arxiv},
eprintclass = {astro-ph},
month = apr,
annote = {8 pages, 3 figures, accepted for publication in MNRAS},
affiliation = {JBO},
doi = {10.1111/j.1365-2966.2005.09005.x},
rating = {0},
date-added = {2011-10-25T10:24:26GMT},
date-modified = {2013-06-15T18:52:08GMT},
abstract = {We present the results of a 430-MHz survey for pulsars conducted during the upgrade to the 305-m Arecibo radio telescope. Our survey covered a total of 1147 square degrees of sky using a drift-scan technique. We detected 33 pulsars, 10 of which were not known prior to the survey observations. The highlight of the new discoveries is PSR J0407+1607, which has a spin period of 25.7 ms, a characteristic age of 1.5 Gyr and is in a 1.8-yr orbit about a low-mass (>0.2 Msun) companion. The long orbital period and small eccentricity (e = 0.0009) make the binary system an important new addition to the ensemble of binary pulsars suitable to test for violations of the strong equivalence principle. We also report on our initially unsuccessful attempts to detect optically the companion to J0407+1607 which imply that its absolute visual magnitude is > 12.1. If, as expected on evolutionary grounds, the companion is an He white dwarf, our non-detection imples a cooling age of least 1 Gyr.

Published in: Mon.Not.Roy.Astron.Soc. 359 (2005) 1524-1530},
url = {http://arxiv.org/abs/astro-ph/0504019v1},
local-url = {file://localhost/Users/siemion/Documents/Papers2/Articles/2005/Lorimer/arXiv%202005%20Lorimer.pdf},
file = {{arXiv 2005 Lorimer.pdf:/Users/siemion/Documents/Papers2/Articles/2005/Lorimer/arXiv 2005 Lorimer.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1111/j.1365-2966.2005.09005.x}}
}

