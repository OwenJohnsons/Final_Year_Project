@article{2011ApJ...732L..12B,
author = {Bower, Geoffrey C},
title = {{Constraining the Rate of Relativistic Jets from Tidal Disruptions Using Radio Surveys}},
journal = {The Astrophysical Journal Letters},
year = {2011},
volume = {732},
number = {1},
