@article{Wex:1996p8768,
author = {Wex, N and Gil, J and Sendyk, M},
title = {{Microlensing of pulsar radiation in the Galactic Center.}},
journal = {Astronomy and Astrophysics},
year = {1996},
volume = {311},
pages = {746},
month = jul,
rating = {0},
date-added = {2011-01-31T10:41:08GMT},
date-modified = {2016-05-18T05:17:59GMT},
abstract = {We investigate the possibility of identifying massive objects (lenses) in the Galactic Center region (GC) by means of pulsar timing. The well known intensity change due to microlensing is found to be less important. For typical stellar masses, the flux magnification can be significant only if the lens passes very close to the pulsar-Earth axis. We show that in the case of a pulsar the varying travel-time delay, which is observable because of the pulsating nature of pulsar radiation, is a much more powerful tool to investigate mass distributions in the GC. We find that a travel-time delay is measurable even for rather large distances between the lens and the pulsar-Earth axis. The time varying travel-time delay can be used to determine the mass of the lens and the ratio of the transverse velocity to the minimum impact parameter. Based on Monte Carlo simulations of synthetic data sets we give the expected accuracies in the parameter determination for various masses. We argue that pulsars found behind the very center of our Galaxy would provide an excellent opportunity to test the mass distribution in the Galactic nucleus, and therefore to distinguish between a super massive black hole (SMBH) and a super-dense star cluster (SDSC) within the central 0.1pc.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=1996A%26A...311..746W&link_type=ABSTRACT},
local-url = {file://localhost/Users/siemion/Documents/Papers2/Articles/1996/Wex/Astronomy%20and%20Astrophysics%201996%20Wex.pdf},
file = {{Astronomy and Astrophysics 1996 Wex.pdf:/Users/siemion/Documents/Papers2/Articles/1996/Wex/Astronomy and Astrophysics 1996 Wex.pdf:application/pdf}},
uri = {\url{papers2://publication/uuid/B288726B-FDAB-4DB7-B5E9-0FDC9C8DA8B4}}
}

@article{Pfahl:2004p8823,
