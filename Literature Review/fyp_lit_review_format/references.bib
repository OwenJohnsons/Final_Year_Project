number = {1},
pages = {589--591},
month = n/a,
affiliation = {CSIRO Australia Telescope National Facility, P.O. Box 76, Epping, NSW, 2121, Australia},
doi = {10.1016/S0094-5765(98)00012-5},
read = {Yes},
