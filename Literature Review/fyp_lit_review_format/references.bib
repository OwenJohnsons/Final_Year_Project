@inproceedings{2014PNAS..111.6871R,
author = {Rein, H and Fujii, Y and Spiegel, D S},
title = {{Some inconvenient truths about biosignatures involving two chemical species on Earth-like exoplanets}},
booktitle = {Proceedings of the National Academy of Sciences},
year = {2014},
pages = {6871--6875},
month = may,
doi = {10.1073/pnas.1401816111},
