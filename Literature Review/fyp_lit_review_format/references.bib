}

@article{2015arXiv150100753C,
author = {Cordes, J M and Wasserman, Ira},
title = {{Supergiant Pulses from Extragalactic Neutron Stars}},
journal = {arXiv.org},
year = {2015},
eprint = {1501.00753},
